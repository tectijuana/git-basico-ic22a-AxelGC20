Ejercicio 56-
lista = [10,80,70,90,100,95,87,60,75,98,72,55,45,98,86,30,99,71,15]
contador=0
promedio=65
    
for i in range(len(lista[:])):
    if promedio>=lista[i]:
        contador=contador+1
        lista.extend([lista[i]])
print("La cantidad de calificaciones menores a 65 son: " + str(contador))

Ejercicio 57-
lista = [180, 172, 178, 185, 190, 195, 192, 200, 210, 190]
mean = sum(lista)/len(lista)
print("el promedio es:",mean)
lista.sort()
print(lista)


Ejercicio 58-
numero=int(input("digite un numero: "))
print("su numero es: ", numero)
print()
numero = int(str(numero)[::-1])
print("Su numero invertido es:", numero)


